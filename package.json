{
  "name": "eminent_domain",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "redux": "redux-devtools --hostname=localhost --port=8081 --open=browser",
    "server": "nodemon",
    "start": "react-native start",
    "start:prod": "HOST=\"https://eminent-domain.herokuapp.com\" react-native start",
    "test": "jest",
    "postinstall": "tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.7.3",
    "@react-navigation/stack": "5.9.0",
    "express": "4.17.1",
    "lodash": "4.17.20",
    "module-alias": "2.2.2",
    "random-js": "2.1.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-extended-stylesheet": "0.12.0",
    "react-native-gesture-handler": "1.8.0",
    "react-native-get-random-values": "1.5.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "1.13.0",
    "react-native-safe-area-context": "3.1.8",
    "react-native-screens": "2.11.0",
    "react-redux": "7.2.1",
    "redux-observable": "1.2.0",
    "redux-socket.io": "1.4.0",
    "rxjs": "6.6.3",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "tsconfig-paths": "3.9.0",
    "typesafe-actions": "5.1.0",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/express": "4.17.8",
    "@types/jest": "25.2.3",
    "@types/node": "14.10.1",
    "@types/react-native": "0.63.18",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.3",
    "@types/redux-socket.io": "1.4.0",
    "@types/remote-redux-devtools": "0.5.4",
    "@types/socket.io": "2.1.11",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-jest": "25.5.1",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "metro-react-native-babel-preset": "0.59.0",
    "nodemon": "2.0.4",
    "react-native-clean-project": "3.4.0",
    "react-test-renderer": "16.13.1",
    "redux-devtools-cli": "1.0.0-4",
    "redux-devtools-extension": "2.13.8",
    "remote-redux-devtools": "0.5.16",
    "ts-node": "9.0.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server",
      "common",
      "typings"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./server/index.ts",
    "ext": "ts, js"
  },
  "engines": {
    "node": "10.16.3"
  },
  "_moduleAliases": {
    "@types": "build/common/types.js",
    "@actions": "build/common/redux/actions",
    "@common": "build/common"
  }
}
